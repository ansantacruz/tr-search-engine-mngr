{
  "name": "tr-search-engine-mngr",
  "version": "1.0.0",
  "description": "Microservice that manages the different products to build up purchases. ",
  "private": true,
  "main": "build/bin/www.js",
  "scripts": {
    "build": "tsc",
    "deploy": "export DEBUG=bdb:* && node ./build/bin/www.js",
    "start": "npm run build && export DEBUG=bdb:* && node ./build/bin/www.js",
    "dev": "nodemon --watch src -e ts,js --exec npm start",
    "test:file": "mocha --timeout 10000 --require ts-node/register",
    "test:only": "npm run test:file -- 'test/**/*.spec.ts'",
    "test": "npm run test:only",
    "coverage": "nyc _mocha --timeout 5000 --exit --require ts-node/register 'test/**/*.spec.ts'",
    "coverage:report": "nyc report --reporter=lcov --reporter=text",
    "coverage:web": "npm run coverage && http-server coverage/",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "npx eslint '*/**/*.{js,ts}' --fix",
    "prettier:all": "prettier \"**/*.{ts,js,json,html,scss,md}\" --write",
    "prettier": "prettier \"**/*.{ts,js,json,html,scss,md}\" --check",
    "fix:all": "npm run prettier:all && npm run lint:fix",
    "sonar:local": "npm run coverage:report && npm run clean && sonar-scanner",
    "debug": "DEBUG=* node ./build/bin/www.js",
    "clean": "rm -rf dist .scannerwork",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
  },
  "keywords": [
    "typescript",
    "express",
    "js",
    "node-js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansantacruz/tr-search-engine-mngr.git"
  },
  "author": "ansantacruz@uan.edu.co",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1130.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.26.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.17.3",
    "express-actuator": "^1.8.1",
    "express-openapi-validate": "^0.6.1",
    "http-status": "^1.5.2",
    "install": "^0.13.0",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "node-fetch": "^2.6.7",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.20.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/chai": "^4.3.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-actuator": "^1.8.0",
    "@types/mocha": "^9.1.1",
    "@types/mock-express-request": "^0.2.0",
    "@types/morgan": "^1.9.3",
    "@types/nock": "^11.1.0",
    "@types/node": "^17.0.31",
    "@types/node-fetch": "^2.5.12",
    "@types/sinon": "^10.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "mock-express-request": "^0.2.2",
    "nock": "^13.2.4",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.6.1",
    "sinon": "^13.0.2",
    "sonarqube-scanner": "^2.8.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  }
}
